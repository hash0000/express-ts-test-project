{
  "name": "aerostat-backend",
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node -r dotenv/config dist/main.js dotenv_config_path=./.env",
    "start:linux": "node -r dotenv/config dist/main.js dotenv_config_path=./.env.linux",
    "start:local": "nodemon -r dotenv/config app/main.ts dotenv_config_path=./.env",
    "start:build": "node -r source-map-support/register dist/main.js",
    "kysely-codegen": "npx kysely-codegen --out-file ./app/common/type/kysely/db.type.ts",
    "run-migration:local": "nodemon -r dotenv/config app/common/helper/runMigration.helper.ts",
    "run-migration:local-js": "node -r dotenv/config dist/common/helper/runMigration.helper.js",
    "run-migration": "nodemon app/common/helper/runMigration.helper.ts",
    "run-migration-js": "node dist/common/helper/runMigration.helper.js"
  },
  "dependencies": {
    "argon2": "^0.40.1",
    "better-sqlite3": "^9.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "kysely-codegen": "^0.11.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engineStrict": true,
  "packageManager": "^npm@8.20.2",
  "engines": {
    "npm": ">=8.20.2",
    "node": ">=16.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "files/*"
    ],
    "ext": "ts, json, yaml"
  }
}
